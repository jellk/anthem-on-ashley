{% block content %} 
	<div class="container">

		<!-- Header -->
		<header>
			<img src="{{'header.jpg' | asset_url}}" alt="" class="img-responsive">	
			<img src="{{'banner.jpg' | asset_url}}" alt="" class="img-responsive">	
		</header>
		
		<!-- Content -->
		<section id="content">
			<!-- Divider -->
			<div class="divider">
			</div>

			<!-- Image -->
			<div class="img-section">
				<div>
					<img src="{{'footer.jpg' | asset_url}}" alt="" class="img-responsive">	
				</div>
		</section>
		
		<!-- Google Map -->
		<section id="google-map">
			<!-- Divider -->
			<div class="divider">
				<p>
					CHECK OUT THE NEIGHBORHOOD
				</p>
			</div>

			<!-- Google Map -->
			<div id="map"></div>
		</section>
		
		<!-- Footer -->
		<footer>
			<div class="row">

				<!-- Contact Form -->
				<div class="col-xs-6">
					<p>SIGN UP FOR LATEST NEWS AND UPDATES</p>
					<form>
					  <div class="form-group">
					    <label for="user-name"></label>
					    <input type="email" class="form-control" id="user-name" placeholder="Your Name">
					    <label for="email-address"></label>
					    <input type="email" class="form-control" id="email-address" placeholder="Your Email Address">
					  </div>
					  <button type="submit" class="btn btn-default submit-btn">SUBMIT</button>
					</form>
					<br>
					<div class="copyrights">
						<p>Copyright &copy; 2015 Anthem on Ashley <br>
							All Rights Reserved | Owned and * Operated by NAP</p>
					</div>
				</div>

				<!--Social Media -->
				<div class="col-xs-5 col-sm-offset-1">
					<p>CONNECT WITH US</p>
					<div class="social-icons">
						<i class="fa fa-facebook-square"></i>
						<i class="fa fa-youtube"></i>
						<i class="fa fa-pinterest"></i>
						<i class="fa fa-twitter"></i>
					</div>
					<br>
					<p>
						1234 Ralph McGill Blvd.
						<br>
						Atlanta, GA 30002
					</p>
					<br>
					<img src="{{'nap-logo.png' | asset_url}}" alt="" class="img-responsive">
				</div>
			</div>
		</footer>
	
	</div>
{% endblock %}